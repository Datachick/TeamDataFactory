{
	"name": "Copy data from on premise SQL Server to Blob",
	"properties": {
		"description": "Copy data from laptop DB to blob\n\nYou can also go to \"Copy Data Tool\" to get the pipeline for more connectors and scenarios.",
		"activities": [
			{
				"name": "LocalADV_to_Blob",
				"description": "Copy data from SQL server to Azure Blob file",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "LaptopADVW"
					},
					{
						"name": "Destination",
						"value": "factoryfiles//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "BusinessEntityID"
								},
								"sink": {
									"name": "BusinessEntityID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Title"
								},
								"sink": {
									"name": "Title",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "FirstName"
								},
								"sink": {
									"name": "FirstName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "MiddleName"
								},
								"sink": {
									"name": "MiddleName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "LastName"
								},
								"sink": {
									"name": "LastName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Suffix"
								},
								"sink": {
									"name": "Suffix",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "JobTitle"
								},
								"sink": {
									"name": "JobTitle",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "TxNUM"
								},
								"sink": {
									"name": "TxNUM",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "PhoneNumberType"
								},
								"sink": {
									"name": "PhoneNumberType",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "EmailAddress"
								},
								"sink": {
									"name": "EmailAddress",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "EmailPromotion"
								},
								"sink": {
									"name": "EmailPromotion",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "AddressLine1"
								},
								"sink": {
									"name": "AddressLine1",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "AddressLine2"
								},
								"sink": {
									"name": "AddressLine2",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "City"
								},
								"sink": {
									"name": "City",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "StateProvinceName"
								},
								"sink": {
									"name": "StateProvinceName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "PostalCode"
								},
								"sink": {
									"name": "PostalCode",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "CountryRegionName"
								},
								"sink": {
									"name": "CountryRegionName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "TerritoryName"
								},
								"sink": {
									"name": "TerritoryName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "TerritoryGroup"
								},
								"sink": {
									"name": "TerritoryGroup",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "SalesQuota"
								},
								"sink": {
									"name": "SalesQuota",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"name": "SalesYTD"
								},
								"sink": {
									"name": "SalesYTD",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"name": "SalesLastYear"
								},
								"sink": {
									"name": "SalesLastYear",
									"type": "Decimal"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SQLServerLaptopSalesPeople",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesPersonsCSV",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "CSV to JSON",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "LocalADV_to_Blob",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true,
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "genres",
									"type": "String"
								},
								"sink": {
									"path": "$['genres']"
								}
							},
							{
								"source": {
									"name": "Rating",
									"type": "String"
								},
								"sink": {
									"path": "$['Rating']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SalesPersonsCSV",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesPersonVIPs",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}